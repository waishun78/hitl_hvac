    "seed": 3,
    "model": null,
    "wrappers": {
        "NormalizeAction": {},
        "NormalizeObservation": {},
        "LoggerWrapper": {
            "logger_class": "sinergym.utils.logger.CSVLogger",
            "flag": true
        }
    },
    "evaluation": {
        "eval_freq": 2,
        "eval_length": 2
    },
    "wandb": {
        "init_params": {
            "project": "sinergym",
            "entity": "alex_ugr"
        },
        "artifact_name": "experiment_PPO",
